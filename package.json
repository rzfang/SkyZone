{
  "name": "sky_zone",
  "version": "0.0.1",
  "description": "Sky Zone",
  "main": "server.js",
  "dependencies": {
    "async": "^2.1.4",
    "sqlite3": "^3.1.13"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-json": "^1.2.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.3",
    "pre-commit": "^1.2.2",
    "uglify-js": "^2.6.2"
  },
  "scripts": {
    "dev": "nodemon -d 3 -e js,scss,html,tag -w SRC/ SRC/RZ-Nd-HTTPServer.js",
    "lint": "eslint --ext .js,.json SRC/*.js",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rzfang/SkyZone.git"
  },
  "keywords": [
    "web",
    "tool"
  ],
  "author": "RZ Fang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rzfang/SkyZone/issues"
  },
  "homepage": "https://github.com/rzfang/SkyZone#readme",
  "pre-commit": [],
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jquery": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "json"
    ],
    "rules": {
      "no-console": [
        "warn",
        {
          "allow": [
            "warn",
            "error"
          ]
        }
      ],
      "no-redeclare": [
        "warn",
        {
          "builtinGlobals": true
        }
      ]
    }
  },
  "eslintIgnore": [
    "SRC/api.js",
    "SRC/include.js"
  ]
}
